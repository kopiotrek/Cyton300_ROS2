<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">



    <xacro:macro name="cyton_robot">
        <xacro:if value="$(arg use_sim)">
            <xacro:cyton_ignition />
        </xacro:if>
        <xacro:unless value="$(arg use_sim)">
            <xacro:cyton_hardware initial_positions_file="$(arg initial_positions_file)"/>
        </xacro:unless>

        <material name="blue">
          <color rgba= "1 0 0 1"/>
        </material>

        <!-- Initial footprint motor used here -->
        <link name="base_footprint">
                <!-- ZMIENIÄ†!!! -->
                <inertial>
                    <mass value="2.92"/>
                    <origin rpy="0 0 0" xyz="-0.025566 -2.88e-05 0.057332"/>
                    <inertia ixx="0.00782229414331" ixy="-1.56191622996e-05" ixz="-0.00126005738123" iyy="0.0109027971813" iyz="1.08233858202e-05" izz="0.0102355503949"/>
                  </inertial>
                <visual>
                    <origin xyz="0 0 1" rpy="0 0 0" />
                    <geometry>
                        <sphere radius="0.001" />
                    </geometry>
                    <material name="blue"/>
                </visual>
                <collision>
                    <origin xyz="0 0 1" rpy="0 0 0" />
                    <geometry>
                        <sphere radius="0.01" />
                    </geometry>
                </collision>
        </link>

        <joint name="base_joint" type="fixed">
                <parent link="base_footprint" />
                <child link="base_link" />
                <dynamics damping="0.5" friction="0.5"/>
                <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        </joint>

        <!-- Base of robot here -->
        <link name="base_link">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/gamma_base.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/gamma_base.stl" />
                    <!-- <box size="0.1 0.1 0.007" /> -->
                </geometry>
                <material name="blue"/>
            </collision>
            <inertial>
                <mass value="0.2" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia ixx="1.1027" iyy="1.1027" izz="1.1027"
                    ixy="0.05" ixz="0.05" iyz="0.05" />
            </inertial>
        </link>

        <!-- First motor located here -->
        <joint name="joint1" type="revolute">
            <parent link="base_link" />
            <child link="link1" />
            <origin xyz="0 0 0.05315" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-2.618" upper="2.618" effort="100.0" velocity="1.0" />
            <axis xyz="0 0 1" />
        </joint>

        <link name="link1">
            <visual>
                <origin xyz="0 0 0" rpy="1.5708 0 3.14159" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint1.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="1.5708 0 3.14159" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint1.stl" />
                </geometry>
            </collision>
            <inertial>
                <mass value="5.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.011208614136379117" ixy="0" ixz="0" 
                    iyy="0.009369607269126357" iyz="0" izz="0.003859341087193736" />
            </inertial>
        </link>

        <!-- Second motor located here -->
        <joint name="joint2" type="revolute">
            <parent link="link1" />
            <child link="link2" />
            <origin xyz="0 0 0.063" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="100.0" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint>

        <link name="link2">
            <visual>
                <origin xyz="0.028 0 0" rpy="0 1.5708 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint2.dae" />
                </geometry>
                <material name="blue"/>             
            </visual>
            <collision>
                <origin xyz="0.028 0 0" rpy="0 1.5708 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint2.stl" />
                </geometry>
            </collision>
            <inertial>
                <mass value="2.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.004978331673653838" ixy="0" ixz="0" 
                    iyy="0.005399428220449068" iyz="0" izz="0.005770766916274131" />
            </inertial>
        </link>

        
        <!-- Third motor located here -->
        <joint name="joint3" type="revolute">
            <parent link="link2" />
            <child link="link3" />
            <origin xyz="0 0 0.089" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="100.0" velocity="1.0" />
            <axis xyz="0 0 1" />
        </joint>
+
        <link name="link3">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 3.92699" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint3.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 3.92699" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint3.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="0.2" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.004185745441913727" ixy="0" ixz="0" 
                    iyy="0.0010369991749477983" iyz="0" izz="0.003815542609643933" />


            </inertial>
        </link>

        <!-- Fourth motor located here -->
        <joint name="joint4" type="revolute">
            <parent link="link3" />
            <child link="link4" />
            <origin xyz="0 0 0.052" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="100.0" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint>

        <link name="link4">
            <visual>
                <!-- Not sure if this is the correct orientation. -->
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint4.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint4.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="0.2" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.0002991427484832406" ixy="0" ixz="0" 
                    iyy="0.0002771226997735077" iyz="0" izz="9.534005603599922e-05" />
            </inertial>
        </link>

        <!-- Fifth motor located here -->
        <joint name="joint5" type="revolute">
            <parent link="link4" />
            <child link="link5" />
            <origin xyz="0 0 0.072" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="100.0" velocity="1.0" />
            <axis xyz="0 -1 0" />
        </joint>

        <link name="link5">
            <visual>
                <origin xyz="0 0 0" rpy="1.5708 0 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint5.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="1.5708 0 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint5.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="0.2" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.004619866095305994" ixy="0" ixz="0" 
                    iyy="0.005877155994627273" iyz="0" izz="0.00722870642358601" />

            </inertial>
        </link>

        <!-- Sixth motor located here -->
        <joint name="joint6" type="revolute">
            <parent link="link5" />
            <child link="link6" />
            <origin xyz="0 0 0.0718" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="100.0" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint>

        <link name="link6">
            <visual>
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint6.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint6.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="0.2" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.002306163215052582" ixy="0" ixz="0" 
                    iyy="0.003544626555053466" iyz="0" izz="0.004174319112083028" />
            </inertial>
        </link>

        <!-- Seventh motor located here -->
        <joint name="joint7" type="revolute">
            <parent link="link6" />
            <child link="link7" />
            <origin xyz="0 0 0.051425" rpy="0 0 1.5708" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-2.61799" upper="2.61799" effort="100.0" velocity="1.0" />
            <axis xyz="0 0 1" />
        </joint>

        <link name="link7">
            <visual>
                <origin xyz="0 0 0" rpy="0 3.14159 1.5708" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint7.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 3.14159 1.5708" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint7.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="0.2" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.0015572863522148085" ixy="0" ixz="0" 
                    iyy="0.0015598712477069795" iyz="0" izz="0.002615797579411179" />
            </inertial>
        </link>

        <!-- Eighth motor located here -->
        <joint name="joint8" type="prismatic">
            <parent link="link7" />
            <child link="link8" />
            <origin xyz="-0.02131 0.00485 0.077225" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-0.002" upper="0.018" effort="0.1" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint>

        <!-- <joint name="joint8" type="prismatic">
            <parent link="joint7" />
            <child link="joint8" />
            <origin xyz="-0.05331 0.00485 0.077225" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-0.001" upper="0.017" effort="100.0" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint> -->

        <link name="link8">
            <visual>
                <origin xyz="0 0 -0.035" rpy="0 0 3.14" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint9_stretched.dae"/>
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 -0.035" rpy="0 0 3.14" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint9_stretched.stl"/>
                </geometry>
            </collision>
            <inertial>
                <mass value="0.02" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.00011683333934843555" ixy="0" ixz="0" 
                    iyy="9.527084112726167e-05" iyz="0" izz="5.193749979697169e-05" />
            </inertial>
        </link>

        <!-- Eighth motor located here -->
        <joint name="joint9" type="prismatic">
            <parent link="link7" />
            <child link="link9" />
            <origin xyz="0.0178 0.00485 0.077225" rpy="0 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-0.018" upper="0.002" effort="0.1" velocity="1.0" />
            <mimic joint="joint8" multiplier="-1"/>
            <axis xyz="1 0 0" />
        </joint>


        <link name="link9">
            <visual>
                <!-- <origin xyz="0 0 -0.03" rpy="0 0 0" /> -->
                <origin xyz="0 0 -0.035" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/visual/joint9_stretched.dae"/>
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 -0.035" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(arg models_dir)/cyton/meshes/collision/joint9_stretched.stl"/>
                </geometry>
            </collision>
            <inertial>
                <mass value="0.02" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.00011683333934843555" ixy="0" ixz="0" 
                    iyy="9.527084112726167e-05" iyz="0" izz="5.193749979697169e-05" />
            </inertial>
        </link>


        <!-- Dummy joint/link to prevent error with mimic joint -->
        <joint name="joint9_mimic" type="fixed">
            <parent link="world" />
            <child link="dummy_mimic_fix" />
        </joint>
        <link name="dummy_mimic_fix"/>
    </xacro:macro>

</robot>