<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">


    <xacro:include filename="$(find cyton_ros2_gazebo)/urdf/cyton_ignition.xacro" />
    <xacro:include filename="$(find cyton_ros2_gazebo)/urdf/cyton_transmission.xacro" />

    <xacro:macro name="cyton_robot">
        <xacro:cyton_transmission />
        <!-- <xacro:cyton_ignition /> -->

        <material name="blue">
          <color rgba= "255 149 0 1"/>
        </material>

        <!-- Initial footprint motor used here -->
        <link name="base_footprint">
                <!-- ZMIENIĆ!!! -->
                <inertial>
                    <mass value="2.92"/>
                    <origin rpy="0 0 0" xyz="-0.025566 -2.88e-05 0.057332"/>
                    <inertia ixx="0.00782229414331" ixy="-1.56191622996e-05" ixz="-0.00126005738123" iyy="0.0109027971813" iyz="1.08233858202e-05" izz="0.0102355503949"/>
                  </inertial>
                <!-- ZMIENIĆ -->
                <visual>
                    <origin xyz="0 0 1" rpy="0 0 0" />
                    <geometry>
                        <sphere radius="0.001" />
                    </geometry>
                    <material name="blue"/>
                </visual>
                <collision>
                    <origin xyz="0 0 1" rpy="0 0 0" />
                    <geometry>
                        <sphere radius="0.01" />
                    </geometry>
                </collision>
        </link>

        <joint name="base_joint" type="fixed">
                <parent link="base_footprint" />
                <child link="base_link" />
                <dynamics damping="0.5" friction="0.5"/>
                <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        </joint>

        <!-- Base of robot here -->
        <link name="base_link">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/gamma_base.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/gamma_base.stl" />
                    <!-- <box size="0.1 0.1 0.007" /> -->
                </geometry>
                <material name="blue"/>
            </collision>
            <inertial>
                <mass value="1.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia ixx="1.1027" iyy="1.1027" izz="1.1027"
                    ixy="0.05" ixz="0.05" iyz="0.05" />
            </inertial>
        </link>

        <!-- First motor located here -->
        <joint name="joint1" type="revolute">
            <parent link="base_link" />
            <child link="shoulder_roll" />
            <origin xyz="0 0 0.05315" rpy="0 0 1.2" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-2.618" upper="2.618" effort="7.0" velocity="1.0" />
            <axis xyz="0 0 1" />
        </joint>

        <link name="shoulder_roll">
            <visual>
                <origin xyz="0 0 0" rpy="1.5708 0 3.14159" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/Shoulder_Roll.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="1.5708 0 3.14159" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/Shoulder_Roll.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="5.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.011208614136379117" ixy="0" ixz="0" 
                    iyy="0.009369607269126357" iyz="0" izz="0.003859341087193736" />
            </inertial>
        </link>

        <!-- Second motor located here -->
        <joint name="joint2" type="revolute">
            <parent link="shoulder_roll" />
            <child link="shoulder_pitch" />
            <origin xyz="0 0 0.063" rpy="1.2 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="7.0" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint>

        <link name="shoulder_pitch">
            <visual>
                <origin xyz="0.028 0 0" rpy="0 1.5708 0" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/Shoulder_Pitch.dae" />
                </geometry>
                <material name="blue"/>             
            </visual>
            <collision>
                <origin xyz="0.028 0 0" rpy="0 1.5708 0" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/Shoulder_Pitch.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="2.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.004978331673653838" ixy="0" ixz="0" 
                    iyy="0.005399428220449068" iyz="0" izz="0.005770766916274131" />
            </inertial>
        </link>
        <!-- Third motor located here -->
        <joint name="joint3" type="revolute">
            <parent link="shoulder_pitch" />
            <child link="elbow_roll" />
            <origin xyz="0 0 0.089" rpy="0 0 1.2" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="7.0" velocity="1.0" />
            <axis xyz="0 0 1" />
        </joint>

        <link name="elbow_roll">
            <visual>
                <origin xyz="0 0 0" rpy="0 0 3.92699" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/Elbow_Roll.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 3.92699" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/Elbow_Roll.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="1.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.004185745441913727" ixy="0" ixz="0" 
                    iyy="0.0010369991749477983" iyz="0" izz="0.003815542609643933" />


            </inertial>
        </link>

        <!-- Fourth motor located here -->
        <joint name="joint4" type="revolute">
            <parent link="elbow_roll" />
            <child link="elbow_pitch" />
            <origin xyz="0 0 0.052" rpy="1.2 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="7.0" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint>

        <link name="elbow_pitch">
            <visual>
                <!-- Not sure if this is the correct orientation. -->
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/Elbow_Pitch.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/Elbow_Pitch.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="1.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.0002991427484832406" ixy="0" ixz="0" 
                    iyy="0.0002771226997735077" iyz="0" izz="9.534005603599922e-05" />
            </inertial>
        </link>

        <!-- Fifth motor located here -->
        <joint name="joint5" type="revolute">
            <parent link="elbow_pitch" />
            <child link="elbow_yaw" />
            <origin xyz="0 0 0.072" rpy="0 -1.2 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="7.0" velocity="1.0" />
            <axis xyz="0 -1 0" />
        </joint>

        <link name="elbow_yaw">
            <visual>
                <origin xyz="0 0 0" rpy="1.5708 0 0" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/Elbow_Yaw.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="1.5708 0 0" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/Elbow_Yaw.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="1.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.004619866095305994" ixy="0" ixz="0" 
                    iyy="0.005877155994627273" iyz="0" izz="0.00722870642358601" />

            </inertial>
        </link>

        <!-- Sixth motor located here -->
        <joint name="joint6" type="revolute">
            <parent link="elbow_yaw" />
            <child link="wrist_pitch" />
            <origin xyz="0 0 0.0718" rpy="1.2 0 0" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-1.8326" upper="1.8326" effort="7.0" velocity="1.0" />
            <axis xyz="1 0 0" />
        </joint>

        <link name="wrist_pitch">
            <visual>
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/Wrist_Pitch.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0.027 0 0" rpy="1.5708 0 1.5708" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/Wrist_Pitch.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="1.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.002306163215052582" ixy="0" ixz="0" 
                    iyy="0.003544626555053466" iyz="0" izz="0.004174319112083028" />
            </inertial>
        </link>

        <!-- Seventh motor located here -->
        <joint name="joint7" type="revolute">
            <parent link="wrist_pitch" />
            <child link="wrist_roll" />
            <origin xyz="0 0 0.051425" rpy="0 0 1.2" />
            <dynamics damping="0.5" friction="0.5"/>
            <limit lower="-2.61799" upper="2.61799" effort="7.0" velocity="1.0" />
            <axis xyz="0 0 1" />
        </joint>

        <link name="wrist_roll">
            <visual>
                <origin xyz="0 0 0" rpy="0 3.14159 1.5708" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/visual/Wrist_Roll.dae" />
                </geometry>
                <material name="blue"/>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 3.14159 1.5708" />
                <geometry>
                    <mesh filename="file://$(find cyton_ros2_gazebo)/meshes/collision/Wrist_Roll.stl" />
                    <!-- <sphere radius="0.001" /> -->
                </geometry>
            </collision>
            <inertial>
                <mass value="1.0" />
                <origin rpy="0 0 0" xyz="0 0 0" />
                <inertia  ixx="0.0015572863522148085" ixy="0" ixz="0" 
                    iyy="0.0015598712477069795" iyz="0" izz="0.002615797579411179" />
            </inertial>
        </link>

        <!-- Eighth motor located here -->
        <!-- <joint name="gripper_joint" type="prismatic">
            <parent link="wrist_roll" />
